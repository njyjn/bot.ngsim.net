{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/worship",
      "destination": "https://worshipbot.vercel.app/worship"
    },
    {
      "source": "/worship/:match*",
      "destination": "https://worshipbot.vercel.app/worship/:match*"
    }
  ],
  "redirects": [
    { "source": "/spend", "destination": "https://spendbot.vercel.app" },
    {
      "source": "/spend/:match*",
      "destination": "https://spendbot.vercel.app/:match*"
    },
    { "source": "/baby", "destination": "https://bb.intra.ngsim.net" },
    { "source": "/inventory", "destination": "https://inv.intra.ngsim.net" }
  ]
}
